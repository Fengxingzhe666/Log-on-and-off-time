<Window x:Class="Log_On_and_Off_time.Sub_Window_02"       
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Log_On_and_Off_time"
        mc:Ignorable="d"
        Title="子窗口02" Height="200" Width="400" WindowStyle="None" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="丁真正在激情演唱中……" Margin="0,0,0,125" FontSize="16" FontWeight="Bold" HorizontalContentAlignment="Center" Background="White"/>
        <Button x:Name="Button_01" Content="暂停" Margin="46,79,228,64" FontWeight="Bold" FontSize="20" Background="Gold" Click="Button_Pause"/>
        <Button x:Name="Button_02" Content="停止" Margin="228,79,46,64" FontWeight="Bold" FontSize="20" Background="Firebrick" Click="Button_Stop"/>
        <ProgressBar x:Name="Progress_Bar1" Margin="46,160,46,16" Value="0"/>
        <TextBlock x:Name="timeText" Margin="159,136,141,28" Background="{x:Null}"  />
    </Grid>
</Window>
